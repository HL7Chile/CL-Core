{
  "resourceType": "StructureDefinition",
  "id": "LocalizacionCoreCh",
  "extension": [
    {
      "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-category",
      "valueString": "Base.Entities"
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-security-category",
      "valueCode": "business"
    }
  ],
  "url": "http://minsal.cl/fhir/core//StructureDefinition/LocalizacionCoreCh",
  "version": "0.22",
  "name": "LocalizacionCH",
  "title": "Perfil de Localización de Objetos e Infraestructura",
  "status": "active",
  "description": "Este perfil determina la ubicación especifica para un evento, objeto o Infraestructura específica. Este perfil permite vinlcular la localización especifica con una organización o espacio físico específico ",
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    },
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Location",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Location",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Location.identifier",
        "path": "Location.identifier",
        "short": "Corresponde a un numero de identificación dado por el miderfarm",
        "definition": "Es el número de identificación la localización de cada recinto de dispensación de fármacos en salud",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Location.identifier.value",
        "path": "Location.identifier.value",
        "short": "número identificador"
      },
      {
        "id": "Location.status",
        "path": "Location.status",
        "short": "\tdraft | active | on-hold | revoked | completed | entered-in-error | unknown",
        "mustSupport": true
      },
      {
        "id": "Location.name",
        "path": "Location.name",
        "short": "Nombre legal de la localizacion",
        "mustSupport": true
      },
      {
        "id": "Location.alias",
        "path": "Location.alias",
        "short": "Nombre de fantasía de la localización",
        "mustSupport": true
      },
      {
        "id": "Location.type",
        "path": "Location.type",
        "short": "tipo de localizacion segun V3 Value SetServiceDeliveryLocationRoleType (Extensible)",
        "mustSupport": true
      },
      {
        "id": "Location.type.coding.system",
        "path": "Location.type.coding.system",
        "patternUri": "http://terminology.hl7.org/ValueSet/v3-ServiceDeliveryLocationRoleType"
      },
      {
        "id": "Location.type.coding.code",
        "path": "Location.type.coding.code",
        "short": "Código del tipo",
        "mustSupport": true
      },
      {
        "id": "Location.type.coding.display",
        "path": "Location.type.coding.display",
        "short": "Glosa del código"
      },
      {
        "id": "Location.telecom",
        "path": "Location.telecom",
        "short": "contactos de la organiazacon",
        "mustSupport": true
      },
      {
        "id": "Location.telecom.use",
        "path": "Location.telecom.use",
        "short": "uso del contacto descrito (home | work | temp | old | mobile)"
      },
      {
        "id": "Location.address",
        "path": "Location.address",
        "mustSupport": true
      },
      {
        "id": "Location.address.line",
        "path": "Location.address.line",
        "short": "Calle o avenida, numero y casa o depto",
        "mustSupport": true
      },
      {
        "id": "Location.address.city",
        "path": "Location.address.city",
        "short": "Campo para Comuna de residencia",
        "mustSupport": true,
        "binding": {
          "strength": "required",
          "valueSet": "http://minsal.cl/fhir/core//ValueSet/CodNacComunas"
        }
      },
      {
        "id": "Location.address.district",
        "path": "Location.address.district",
        "short": "Campo para Provincia de Residencia",
        "mustSupport": true,
        "binding": {
          "strength": "required",
          "valueSet": "http://minsal.cl/fhir/core//ValueSet/CodProv"
        }
      },
      {
        "id": "Location.address.state",
        "path": "Location.address.state",
        "short": "Campo para Provincia de Región",
        "mustSupport": true,
        "binding": {
          "strength": "required",
          "valueSet": "http://minsal.cl/fhir/core//ValueSet/CodReg"
        }
      },
      {
        "id": "Location.address.country",
        "path": "Location.address.country",
        "short": "Campo para País de Residencia",
        "mustSupport": true,
        "binding": {
          "strength": "required",
          "valueSet": "http://minsal.cl/fhir/core//ValueSet/DOCType-V2-0203"
        }
      },
      {
        "id": "Location.position",
        "path": "Location.position",
        "short": "Posición física del objeto infraestructura o evento en longitud y latitud",
        "mustSupport": true
      },
      {
        "id": "Location.position.longitude",
        "path": "Location.position.longitude",
        "short": "Longitud valor decimal en WGS84 datum (World Geodetic System 1984)",
        "mustSupport": true
      },
      {
        "id": "Location.position.latitude",
        "path": "Location.position.latitude",
        "short": "Latitud valor decimal en formato WGS84 datum (World Geodetic System 1984)",
        "mustSupport": true
      },
      {
        "id": "Location.managingOrganization",
        "path": "Location.managingOrganization",
        "short": "Organización a la cual la ubicación pertenece si es el caso. Esta relación se hace en base a una referencia a una Orgaización"
      },
      {
        "id": "Location.managingOrganization.reference",
        "path": "Location.managingOrganization.reference",
        "short": "Orgaización de referencia"
      },
      {
        "id": "Location.managingOrganization.display",
        "path": "Location.managingOrganization.display",
        "short": "Nombre en Texto libre de la Orgaización de referencia"
      },
      {
        "id": "Location.hoursOfOperation",
        "path": "Location.hoursOfOperation",
        "short": "horas de operación disponibles en la cual esta disponble la ubicación",
        "mustSupport": true
      }
    ]
  }
}
